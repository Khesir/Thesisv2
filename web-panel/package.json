{
  "name": "web-panel",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:dev": "docker compose -f ../docker-compose.yml up -d",
    "docker:dev:down": "docker compose -f ../docker-compose.yml down",
    "docker:prod": "docker compose -f ../docker-compose.prod.yml up -d",
    "db:migrate": "npx tsx scripts/migration-runner.ts",
    "db:migrate:down": "npx tsx scripts/migration-runner.ts down",
    "db:seed": "npx tsx scripts/seed.ts",
    "electron:dev": "concurrently -k \"next dev\" \"tsc -p electron/tsconfig.json && electron .\"",
    "build:scripts": "tsc -p scripts/tsconfig.json",
    "build:electron": "next build && tsc -p electron/tsconfig.json && npm run build:scripts",
    "electron:build": "npm run build:electron && electron-builder",
    "build:python": "python ../build_python.py",
    "start:prod": "set ELECTRON_PACKAGED=true&& set RESOURCES_PATH=%cd%&& node .next/standalone/server.js"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "mongoose": "^9.1.6",
    "next": "^15.5.12",
    "next-themes": "^0.4.6",
    "node-cache": "^5.1.2",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "^14.4.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "electron": "^35.7.5",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "^15.5.12",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
