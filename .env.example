# LLM Provider Configuration
# Configure at least one provider for the system to work

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini API
GOOGLE_API_KEY=your_google_api_key_here

# Ollama Configuration (local, no API key needed)
# Make sure Ollama is installed and running
# Default URL: http://localhost:11434
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=llama3.1

# Optional: OpenAI API key for future use
# OPENAI_API_KEY=your_openai_key_here
GOOGLE_API_KEY=
MONGODB_URI=mongodb://localhost:27017/thesis_panel
MONGODB_NAME=thesis_panel
